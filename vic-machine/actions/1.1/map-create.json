{
   "key_value": {
      "--name": ".vch.name",
      "--compute-resource": ".vch.compute.vsphere_resource",
      "--memory": ".vch.compute.memory.limit_mb",
      "--memory-reservation": ".vch.compute.memory.reservation_mb",
      "--memory-shares": ".vch.compute.memory.shares",
      "--cpu": ".vch.compute.cpu.limit_mhz",
      "--cpu-reservation": ".vch.compute.cpu.reservation_mhz",
      "--cpu-shares": ".vch.compute.cpu.shares",
      "--bridge-network": ".vch.network.bridge.port_group",
      "--bridge-network-range": ".vch.network.bridge.ip_range",
      "--public-network": ".vch.network.public.port_group",
      "--public-network-ip": ".vch.network.public.vch_static_ip",
      "--public-network-gateway": ".vch.network.public.vch_gateway",
      "--management-network": ".vch.network.management.port_group",
      "--management-network-ip": ".vch.network.management.vch_static_ip",
      "--management-network-gateway": ".vch.network.management.vch_gateway",
      "--client-network": ".vch.network.client.port_group",
      "--client-network-ip": ".vch.network.client.vch_static_ip",
      "--client-network-gateway": ".vch.network.client.vch_gateway",
      "--dns-server": ".vch.network.dns_server",
      "--image-store": ".vch.storage.image_store",
      "--base-image-size": ".vch.storage.base_image_size",
      "--cert-path": ".vch.auth.cert_path",
      "--certificate-key-size": ".vch.auth.keygen.cert_size",
      "--tls-cname": ".vch.auth.keygen.tls_cname",
      "--key": ".vch.auth.server.key_file",
      "--cert": ".vch.auth.server.cert_file",
      "--endpoint-cpu": ".vch.endpoint_vm.vcpus",
      "--endpoint-memory": ".vch.endpoint_vm.memory_mb",
      "--target": ".vcenter.address_datacenter",
      "--thumbprint": ".vcenter.credentials.thumbprint",
      "--user": ".vcenter.credentials.admin_user",
      "--password": ".vcenter.credentials.admin_password",
      "--ops-user": ".vcenter.credentials.vch_user",
      "--ops-password": ".vcenter.credentials.vch_password",
      "--http-proxy": ".registry.image_fetch_proxy.http",
      "--https-proxy": ".registry.image_fetch_proxy.https",
      "--debug": ".vic_machine.debug",
      "--timeout": ".vic_machine.timeout",
      "--appliance-iso": ".vic_machine.appliance_iso",
      "--bootstrap-iso": ".vic_machine.bootstrap_iso"
   },
   "bool": {
      "--no-tls": ".vch.auth.client.no_tls",
      "--no-tlsverify": ".vch.auth.client.no_tls_verify",
      "--use-rp": ".vcenter.use_resource_pool"
   },
   "array": {
      "--volume-store": ".vch.storage.volume_stores",
      "--tls-ca": ".vch.auth.client.tls_ca",
      "--organization": ".vch.auth.keygen.organization",
      "--insecure-registry": ".registry.insecure_registries",
      "--registry-ca": ".registry.registry_ca_files"
   },
   "container_network": {
      "query": ".vch.network.container | .[] | .port_group as $port_group | ",
      "--container-network": "(if (.alias | length) > 0 then ($port_group+':'+.alias) else $port_group end)",
      "--container-network-ip-range": "(if (.ip_range | length) > 0 then ($port_group+':'+.ip_range) else .ip_range end)",
      "--container-network-gateway": "(if (.gateway | length) > 0 then ($port_group+':'+.gateway) else .gateway end)",
      "--container-network-dns": "(if (.dns | length) > 0 then ($port_group+':'+.dns) else .dns end)"
   }
}

