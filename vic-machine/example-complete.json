         },
         "management": {
            "port_group": "ManagementNetwork",
            "vch_static_ip": "10.0.2.2/24",
            "vch_gateway": "10.1.0.0/16,10.2.0.0/16:10.0.0.1"
         },
         "client": {
            "port_group": "ClientNetwork",
            "vch_static_ip": "10.0.0.2/24",
            "vch_gateway": "10.1.0.0/16,10.2.0.0/16:10.0.0.1"
         },
         "container": [
            {
               "port_group": "vsphere-net-dhcp"
